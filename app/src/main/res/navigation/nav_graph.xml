<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@+id/nav_home">

    <fragment
        android:id="@+id/nav_home"
        android:name="com.surromo.millionaire.ui.fragment.home.HomeFragment"
        android:label="@string/app_name"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_nav_home_to_nav_login"
            app:destination="@id/nav_login" />
    </fragment>

    <fragment
        android:id="@+id/nav_moment"
        android:name="com.surromo.millionaire.ui.fragment.moment.MomentFragment"
        android:label="@string/moment"
        tools:layout="@layout/fragment_moment" />

    <fragment
        android:id="@+id/nav_friend"
        android:name="com.surromo.millionaire.ui.fragment.friend.FriendFragment"
        android:label="@string/friend"
        tools:layout="@layout/fragment_friend" />

    <fragment
        android:id="@+id/nav_user"
        android:name="com.surromo.millionaire.ui.fragment.user.UserFragment"
        android:label="@string/user"
        tools:layout="@layout/fragment_user" >
        <action
            android:id="@+id/action_user_to_info"
            app:destination="@id/nav_info"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_pay"
            app:destination="@id/nav_pay"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_homepage"
            app:destination="@id/nav_homepage"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_order"
            app:destination="@id/nav_order"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_trade"
            app:destination="@id/nav_trade"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_identification"
            app:destination="@id/nav_identification"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_setting"
            app:destination="@id/nav_setting"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_user_to_spread"
            app:destination="@id/nav_spread"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
    </fragment>

    <fragment
        android:id="@+id/nav_login"
        android:name="com.surromo.millionaire.ui.fragment.login.LoginFragment"
        android:label="@string/login"
        tools:layout="@layout/fragment_login" >
    </fragment>
    <fragment
        android:id="@+id/nav_info"
        android:name="com.surromo.millionaire.ui.fragment.info.InfoFragment"
        android:label="@string/info"
        tools:layout="@layout/fragment_info">
        <action
            android:id="@+id/action_info_to_avatar"
            app:destination="@id/nav_avatar"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_nickname"
            app:destination="@id/nav_nickname"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_account"
            app:destination="@id/nav_account"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_gender"
            app:destination="@id/nav_gender"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_region"
            app:destination="@id/nav_region"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_profile"
            app:destination="@id/nav_profile"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
        <action
            android:id="@+id/action_info_to_qr_code"
            app:destination="@id/nav_qr_code"
            app:enterAnim="@anim/slide_right_in"
            app:exitAnim="@anim/slide_left_out"
            app:popEnterAnim="@anim/slide_left_in"
            app:popExitAnim="@anim/slide_right_out"/>
    </fragment>
    <fragment
        android:id="@+id/nav_pay"
        android:name="com.surromo.millionaire.ui.fragment.pay.PayFragment"
        android:label="@string/pay"
        tools:layout="@layout/fragment_pay"/>
    <fragment
        android:id="@+id/nav_homepage"
        android:name="com.surromo.millionaire.ui.fragment.homepage.HomepageFragment"
        android:label="@string/homepage"
        tools:layout="@layout/fragment_homepage"/>
    <fragment
        android:id="@+id/nav_order"
        android:name="com.surromo.millionaire.ui.fragment.order.OrderFragment"
        android:label="@string/order"
        tools:layout="@layout/fragment_order"/>
    <fragment
        android:id="@+id/nav_trade"
        android:name="com.surromo.millionaire.ui.fragment.trade.TradeFragment"
        android:label="@string/trade"
        tools:layout="@layout/fragment_trade"/>
    <fragment
        android:id="@+id/nav_identification"
        android:name="com.surromo.millionaire.ui.fragment.identification.IdentificationFragment"
        android:label="@string/identification"
        tools:layout="@layout/fragment_identification"/>
    <fragment
        android:id="@+id/nav_setting"
        android:name="com.surromo.millionaire.ui.fragment.setting.SettingFragment"
        android:label="@string/setting"
        tools:layout="@layout/fragment_setting"/>
    <fragment
        android:id="@+id/nav_spread"
        android:name="com.surromo.millionaire.ui.fragment.spread.SpreadFragment"
        android:label="@string/spread"
        tools:layout="@layout/fragment_spread"/>
    <fragment
        android:id="@+id/nav_avatar"
        android:name="com.surromo.millionaire.ui.fragment.avatar.AvatarFragment"
        android:label="@string/avatar"
        tools:layout="@layout/fragment_avatar"/>
    <fragment
        android:id="@+id/nav_nickname"
        android:name="com.surromo.millionaire.ui.fragment.nickname.NicknameFragment"
        android:label="@string/nickname"
        tools:layout="@layout/fragment_nickname"/>
    <fragment
        android:id="@+id/nav_account"
        android:name="com.surromo.millionaire.ui.fragment.account.AccountFragment"
        android:label="@string/account"
        tools:layout="@layout/fragment_account"/>
    <fragment
        android:id="@+id/nav_gender"
        android:name="com.surromo.millionaire.ui.fragment.gender.GenderFragment"
        android:label="@string/account"
        tools:layout="@layout/fragment_gender"/>
    <fragment
        android:id="@+id/nav_region"
        android:name="com.surromo.millionaire.ui.fragment.region.RegionFragment"
        android:label="@string/region"
        tools:layout="@layout/fragment_region"/>
    <fragment
        android:id="@+id/nav_profile"
        android:name="com.surromo.millionaire.ui.fragment.profile.ProfileFragment"
        android:label="@string/profile"
        tools:layout="@layout/fragment_profile"/>
    <fragment
        android:id="@+id/nav_qr_code"
        android:name="com.surromo.millionaire.ui.fragment.qrcode.QrCodeFragment"
        android:label="@string/qr_code"
        tools:layout="@layout/fragment_qr_code"/>
</navigation>